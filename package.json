{
  "name": "altr-lending",
  "license": "UNLICENSED",
  "scripts": {
    "prepare-testnet": "source .env.development && mustache config/amoy.json subgraph.template.yaml > subgraph.yaml",
    "prepare-mainnet": "bash -c '. .env.production && mustache config/polygon.json subgraph.template.yaml > subgraph.yaml'",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy-mainnet": "source .env.production && graph deploy --node https://api.thegraph.com/deploy/ $NAME",
    "deploy-testnet": "source .env.development && graph deploy --node https://api.thegraph.com/deploy/ $NAME",
    "deploy-testnet-alchemy": "source .env.development && graph deploy $NAME --node https://subgraphs.alchemy.com/api/subgraphs/deploy --deploy-key $TOKEN --ipfs https://ipfs.satsuma.xyz",
    "deploy-mainnet-alchemy": "bash -c '. .env.production && graph deploy $NAME --node https://subgraphs.alchemy.com/api/subgraphs/deploy --deploy-key $TOKEN --ipfs https://ipfs.satsuma.xyz'",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.48.0",
    "@graphprotocol/graph-ts": "0.29.1"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0",
    "mustache": "^4.2.0"
  }
}